<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="bpmn" nsURI="http://bpmnmodelling/bpmn" nsPrefix="bpmn">
  <eClassifiers xsi:type="ecore:EClass" name="FlowObject" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingFlowObjects" upperBound="-1"
        eType="#//FlowObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingFlowObjects" upperBound="-1"
        eType="#//FlowObject"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Task" eSuperTypes="#//FlowObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingFlowObjects" lowerBound="1"
        eType="#//FlowObject" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingFlowObjects" lowerBound="1"
        eType="#//FlowObject" derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Gateway" eSuperTypes="#//FlowObject">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//GatewayType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Process" eSuperTypes="#//FlowObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="flowObjects" lowerBound="1"
        upperBound="-1" eType="#//FlowObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="GatewayType">
    <eLiterals name="Parallel"/>
    <eLiterals name="Exclusive"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WorkflowModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="flowObjects" upperBound="-1"
        eType="#//FlowObject" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subProcess" upperBound="-1"
        eType="#//Process" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TestClass" eSuperTypes="#//FlowObject">
    <eStructuralFeatures xsi:type="ecore:EReference" eType="#//FlowObject" derived="true"/>
  </eClassifiers>
</ecore:EPackage>
