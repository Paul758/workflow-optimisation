<?xml version="1.0" encoding="UTF-8"?>
<workflow:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:workflow="http://bpmnmodelling/workflow">
  <informationObjects
      xsi:type="workflow:Task"
      name="T1"
      isEncapsulatedBy="//@activities.0"
      sink="//@informationObjects.4"/>
  <informationObjects
      xsi:type="workflow:Task"
      name="T2"
      isEncapsulatedBy="//@activities.0"
      sink="//@informationObjects.4"/>
  <informationObjects
      xsi:type="workflow:Task"
      name="T3"
      isEncapsulatedBy="//@activities.0"
      sink="//@informationObjects.4"/>
  <informationObjects
      xsi:type="workflow:Task"
      name="T4"
      isEncapsulatedBy="//@activities.0"
      sink="//@informationObjects.4 //@informationObjects.5"/>
  <informationObjects
      xsi:type="workflow:Task"
      name="T5"
      isEncapsulatedBy="//@activities.1"
      source="//@informationObjects.0 //@informationObjects.1 //@informationObjects.2 //@informationObjects.3"/>
  <informationObjects
      xsi:type="workflow:Task"
      name="T6"
      isEncapsulatedBy="//@activities.2"
      source="//@informationObjects.3"/>
  <activities
      name="A1"
      encapsulates="//@informationObjects.0 //@informationObjects.1 //@informationObjects.2 //@informationObjects.3"/>
  <activities
      name="A2"
      encapsulates="//@informationObjects.4"/>
  <activities
      name="A3"
      encapsulates="//@informationObjects.5"/>
</workflow:Workflow>
